/*搜索 开始*/

.nav-wrap {
  position: fixed;
  width: 100%;
  top: 0;
  background: #fff;
  /* background-color: black; */
  color: #000;
  /* z-index: 0; */
}

.tit_seabox {
  width: calc(100% - 64rpx);
  /* background-color: yellowgreen; */
  box-sizing: border-box;
  padding: 0 14rpx;
  display: flex;
  align-items: center;
  overflow: hidden;
}

.tit_seabox_bar {
  width: calc(100% - 16px);
  height: 30px;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  border-radius: 15px;
  background: #f5f5f5;
  padding-left: 16px;
}

.tit_seabox input {
  line-height: 30px;
  font-size: 14px;
  width: 100%;
  margin-right: 16px;
}
